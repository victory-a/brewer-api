{
  "name": "brewer-api",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon ./src/server.ts & yarn prisma-studio ",
    "build": "tsc",
    "start": "npm run build && node build/server.js",
    "migrate": "cross-env MIGRATION_NAME=npx prisma migrate dev --name $MIGRATION_NAME",
    "prepare": "husky install",
    "prisma-studio": "npx prisma studio",
    "seed-data": "node ./src/scripts/seeder.ts -i",
    "unseed-data": "node ./src/scripts/seeder.ts -d"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.0",
    "lint-staged": "14.0.1",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "ts-node": "^10.9.1",
    "typescript": "*"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.449.0",
    "@prisma/client": "^5.5.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "http-status": "^1.7.3",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "xss-clean": "^0.1.4"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
